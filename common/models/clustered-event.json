{
  "name": "ClusteredEvent",
  "base": "PersistedModel",
  "strict": "throw",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {}
  },
  "properties": {
    "uuid": {
      "type": "string",
      "required": false
    },
    "zoomLevel": {
      "type": "number",
      "required": true
    },
    "coordinates": {
      "type": [
        "geopoint"
      ],
      "required": true
    },
    "centerPoint": {
      "type": "geopoint",
      "required": true
    },
    "startTime": {
      "type": "date",
      "required": true
    },
    "endTime": {
      "type": "date",
      "required": true
    },
    "childId": {
      "type": "string"
    },
    "clusterType": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
