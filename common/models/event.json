{
  "name": "Event",
  "base": "PersistedModel",
  "idInjection": true,
  "strict": "throw",
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "coordinates": {
      "type": ["GeoPoint"],
      "required": true
    },
    "name": {
      "type": "string",
      "description": "A user-defined, identifiable name"
    },
    "eventType": {
      "type": "string",
      "default": "hashtag",
      "description": "Ex. hashtag, user-defined, etc."
    },
    "eventTime": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
