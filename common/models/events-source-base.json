{
  "name": "EventsSourceBase",
  "base": "PersistedModel",
  "strict": "throw",
  "idInjection": true,
  "indexes": {
    "event_id_unique": {
      "keys": {
        "event_id": 1,
        "event_source": 1
      },
      "options": {
        "unique": true
      }
    },
    "post_date_idx": {
      "keys": {
        "post_date": 1
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "event_id": {
      "type": "string",
      "required": true
    },
    "num_posts": {
      "type": "number"
    },
    "source_data": {
      "type": [
        "string"
      ]
    },
    "event_source": {
      "type": "string",
      "required": true
    },
    "indexed_date": {
      "type": "date",
      "required": true
    },
    "post_date": {
      "type": "date",
      "required": true
    },
    "lat": {
      "type": "number",
      "required": true
    },
    "lng": {
      "type": "number",
      "required": true
    },
    "bounding_box": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
