{
  "name": "genie-ui",
  "description": "GENIE User Interface",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "4.4.x",
    "npm": "2.14.x"
  },
  "homepage": "https://github.com/sotera/genie-ui",
  "author": "Justin Lueders <justin.lueders@soteradefense.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sotera/genie-ui"
  },
  "bugs": {
    "url": "https://github.com/sotera/genie-ui/issues"
  },
  "scripts": {
    "postinstall": "npm run build:all",
    "dev": "parallelshell 'npm run server:watch' 'npm run build:watch'",
    "clean": "rm -r client/app/build/*",
    "build:env": "mkdir -p client/app/build/js && ./script/env.js > client/app/build/js/env.js",
    "build:all": "npm run build:env && npm run build:js",
    "build:watch": "npm run watch:js",
    "build:js": "babel client/app/js --source-maps --out-dir client/app/build/js",
    "watch:js": "watch 'npm run build:js' client/app/js",
    "server:watch": "nodemon server/server.js -w server -w common -i server/node-red -i server/datasources.json --ext js,json"
  },
  "main": "server/server.js",
  "dependencies": {
    "JSONStream": "^1.0.7",
    "api-check": "^7.5.5",
    "async": "^1.4.2",
    "body-parser": "1.14.1",
    "cookie-parser": "1.4.0",
    "debug": "2.2.0",
    "deep-extend": "^0.4.0",
    "density-clustering": "^1.3.0",
    "dotenv": "^1.2.0",
    "elasticsearch": "^10.1.0",
    "errorhandler": "1.4.2",
    "event-stream": "^3.3.2",
    "express": "^4.13.3",
    "express-session": "1.12.1",
    "get-env": "0.4.0",
    "glob-fs": "^0.1.6",
    "jade": "1.11.0",
    "json-stable-stringify": "^1.0.0",
    "jszip": "^2.5.0",
    "lodash": "3.10.1",
    "loopback": "~2.23.0",
    "loopback-boot": "~2.14.0",
    "loopback-component-storage": "~1.5.1",
    "loopback-connector-es": "^1.0.3",
    "loopback-connector-mongodb": "~1.13.0",
    "loopback-datasource-juggler": "~2.41.1",
    "moment": "^2.10.6",
    "node-kmeans": "0.0.1",
    "node-red": "^0.12.1",
    "performance-now": "^0.2.0",
    "random-js": "^1.0.8",
    "request": "^2.65.0",
    "request-json": "^0.5.5",
    "serve-favicon": "2.3.0",
    "showdown": "1.3.0",
    "socket.io": "^1.4.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.3",
    "babel-cli": "~6.9.0",
    "babel-preset-es2015": "~6.9.0"
  },
  "optionalDependencies": {
    "loopback-explorer": "2.0.2"
  },
  "devDependencies": {
    "jsonfile": "~2.2.3",
    "nodemon": "~1.9.2",
    "parallelshell": "~2.0.0",
    "watch": "~0.18.0"
  }
}
